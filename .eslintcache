n: column;\n      position: absolute;\n      top:-65%;\n      left: -25%;\n      p {\n        margin: 0;\n      }\n      img {\n        height: 10px;\n        width: 20px;\n        position: absolute;\n        bottom:-8px;\n        left: calc(50% - 20px);\n      }\n    }\n    &:hover {\n      .logo-description {\n      display: flex;\n    }\n    }\n  }\n`;\n\nconst Outils = ({ language, onLanguageClick }) => {\n\n  const title = language ==='fr' ? 'Mes Outils' : 'My Tools'\n\n  const sections = ToolsItems.map((section) => {\n\n    const logos = section.logos.map((item) => {\n      return (\n        <div className='logos'>\n          <img src={process.env.PUBLIC_URL + `/${item.logo}`} alt={item.label}/>\n          <div className='logo-description'>\n            <p>{item.label}</p>\n            <img src={process.env.PUBLIC_URL + `/bottom-arrow.png`} alt={item.label}/>\n          </div>\n        </div>\n      );\n    })\n\n    const title = language === 'fr' ? section.label : section.label_en;\n\n    return (\n      <Section>\n        <div className='title'><h2>{title}</h2></div>\n        {logos}\n      </Section>\n    );\n  });\n\n  return (\n    <Wrapper style={{backgroundColor: '#fff'}}>\n      <MainTitle>{title}</MainTitle>\n      <Content>\n        {sections}\n      </Content>\n    </Wrapper>\n  );\n};\n\nexport default Outils;\n","/Users/charlesdelannoy/code/Charles-Delannoy/portfolio/src/components/pages/Formations.js",["131"],"import React, { Fragment } from 'react';\n// My components\nimport MainTitle from '../styled/MainTitle';\nimport Wrapper from '../styled/Wrapper';\nimport FormationItems from './pagesComponents/FormationItems';\n// Media Queries\nimport { mediaQueries } from \"../mediaQueries/MediaQueries\"\n// External librairies\nimport styled from 'styled-components';\n\n// const Content = styled.div`\n//   height: calc(100vh - 200px - 5%);\n//   display: grid;\n//   grid-template-columns: repeat(2, 1fr);\n//   grid-template-rows: repeat(2, 1fr);\n//  